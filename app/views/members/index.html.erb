<p id="notice"><%= notice %></p>

<h1>Members</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Nick</th>
      <th>Birth</th>
      <th>From</th>
      <th>Tall</th>
      <th>Hobby</th>
      <th>Skill</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @members.each do |member| %>
      <tr>
        <td><%= member.name %></td>
        <td><%= member.nick %></td>
        <td><%= member.birth %></td>
        <td><%= member.from %></td>
        <td><%= member.tall %></td>
        <td><%= member.hobby %></td>
        <td><%= member.skill %></td>
        <!-- <td><%= link_to 'Show', member %></td> -->
        <!-- <td><%= link_to 'Edit', edit_member_path(member) %></td> -->
        <!-- <td><%= link_to 'Destroy', member, method: :delete, data: { confirm: 'Are you sure?' } %></td> -->
      </tr>
    <% end %>
  </tbody>
</table>

